{
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "tdstudioshq.com"
        }
      ],
      "destination": "https://www.tdstudioshq.com/:path*",
      "permanent": true
    }
  ],
  "rewrites": [
    { "source": "/candyman", "destination": "/index.html" },
    { "source": "/linktest", "destination": "/index.html" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, s-maxage=0, must-revalidate"
        },
        {
          "key": "X-Cache-Bust",
          "value": "2025-09-30-20:00"
        },
        {
          "key": "CDN-Cache-Control",
          "value": "no-cache"
        },
        {
          "key": "Vercel-CDN-Cache-Control",
          "value": "no-cache"
        }
      ]
    }
  ]
}